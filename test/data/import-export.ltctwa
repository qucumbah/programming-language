// Compiles to:
// (func
//   $exportFunc
//   (export "exportFunc")
//   (result i32)
// ...
export func exportFunc(): i32 {
  return 15;
}

// Compiles to:
// (import
//   "namespace"
//   "specifier"
//   (func
//     $importFunc
//   )
// )
import(namespace::specifier) func importFunc(): void;

